[ISMIP-HOM-TEST]
# Test A from ISMIP-HOM higher-order test suite
# see: Pattyn et al., The Cryosphere, 2, 95â€“108, 2008.
# www.the-cryosphere.net/2/95/2008

[parameters]
flow_factor = 1

[CF output]
variables = uvel vvel wvel tau_xz tau_yz
frequency = 1
name = dome.out.nc

[grid]
upn = 2
ewn = 1024
nsn = 1024
dew = 40
dns = 40
sigma_builtin = 1

[time]
tstart = 0.
tend = 0.
dt = 1.

[CF default]
comment = forced upper kinematic BC
title = diagnostic solution for parabolic blob of ice

[options]
flow_law = 2            # constant and uniform rate factor
periodic_ew = 1         # doubly periodic lateral boundary conditions
periodic_ns = 1
evolution = 3

[ho_options]
diagnostic_scheme = 1
which_ho_babc = 4
which_ho_sparse = 1
which_ho_efvs = 0

[CF input]
name = dome.1024.nc
time = 1


[external_dycore_options]
external_dycore_type = 0
dycore_input_file = inputs.dome.0032

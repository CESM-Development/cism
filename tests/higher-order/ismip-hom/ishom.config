[ISMIP-HOM-TEST]
# Test from ISMIP-HOM higher-order test suite
# see: Pattyn et al., The Cryosphere, 2, 95â€“108, 2008.
# www.the-cryosphere.net/2/95/2008
# http://homepages.ulb.ac.be/~fpattyn/ismip/ismiphom.pdf

[grid]
upn = 11
ewn = 40    
nsn = 40
dew = 4000   
dns = 4000   

[time]
tstart = 0.
tend = 0.
dt = 1.

[options]
dycore = 1              # 0 = glide, 1 = glam, 2 = glissade
flow_law = 0            # 0 = constant, 2 = Paterson-Budd
evolution = 3           # 3 = remapping 
temperature = 0         # 1 = prognostic, 3 = enthalpy

[ho_options]
which_ho_babc = 4       # 4 = no-slip at bed; 5 = beta field from .nc input
which_ho_efvs = 2       # 0 = constant, 2 = nonlinear eff. visc. w/ n=3
which_ho_sparse = 4     # 1 = SLAP GMRES, 3 = glissade parallel PCG, 4 = Trilinos for linear solver
which_ho_nonlinear = 1  # 0 = Picard, 1 = JFNK

[parameters]
flow_factor = 1.

[CF default]
comment = generated by runGlimmer.py
title = ISMIP-HOM Experiment 

[CF input]
name = ???
time = 1

[CF output]
variables = uvel vvel uvel_icegrid vvel_icegrid topg thk usurf wvel
frequency = 1
name = ???


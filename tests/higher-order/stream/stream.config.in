[STREAM-TEST ]

[grid]
upn = 2
ewn = ?
nsn = ?
dew = ?
dns = ?

[time]
tstart = 0.0
tend = 0.0
dt = 1.0 

[options]
dycore = 2              # 1 = glam, 2 = glissade
flow_law = 0            # isothermal
evolution = 3

[ho_options]
which_ho_babc = 2       # till yield stress map from .nc input (2=Picard; 7=Newton)
which_ho_efvs = 2       # nonlinear eff. visc. w/ n=3
which_ho_sparse = 3     # 1 = SLAP GMRES, 3 = glissade parallel PCG, 4 = Trilinos for linear solver
which_ho_nonlinear = 0  # 0=Picard, 1=JFNK

[parameters]
flow_factor = 1.
periodic_offset_ew = ?

[CF default]
comment = none
title = simple ice stream - yield stress set for analytical solution

[CF input]
name = stream.nc 
time = 1

[CF output]
variables = usrf topg thk flwa uvel vvel velnorm beta dynbcmask tauf
frequency = 100
name = stream.out.nc
xtype = double


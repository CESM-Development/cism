[ISMIP-HOM-TEST]

[parameters]
flow_factor = 1

[CF output]
variables = artm beta topg thk uvel vvel flwa velnorm temp tau_xz tau_yz
frequency = 1
name = data/ishom.c.80km.PIC.out.nc

[grid]
upn = 11
ewn = 41
nsn = 41
dew = 2000
dns = 2000
sigma_builtin = 1

[time]
tstart = 0.
tend = 0.
dt = 1.

[CF default]
comment = generated by runISMIPHOM.py
title = ISMIP-HOM Experiment C

[options]
dycore = 1              # SIA=0; 1st-order = 1
flow_law = 2            # temp dependent = 0; constant/uniform rate factor = 2
evolution = 3           # incremental remapping = 3
temperature = 3         # none = 0; incremental remapping = 3

[ho_options]
diagnostic_scheme = 1   # Payne/Price 1st-order dynamics = 1
which_ho_babc = 5       # periodic basal traction param. from .nc input file = 5
which_ho_efvs = 0       # nonlinear eff. visc. = 0
which_ho_sparse = 4     # serial SLAP GMRES = 1; Trlinos = 4
which_ho_nonlinear = 0  # Picard = 0; JFNK = 1

[CF input]
name = data/ishom.c.80km.nc
time = 1

